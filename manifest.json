{
  "manifest_version": 2,
  "name": "Sidenav",
  "version": "1.0",
  "description": "Add a button to the right and left side of the page to navigate between page.",
  "icons": {
    "48": "./icons/icon48x48.png",
    "128": "./icons/icon128x128.png",
    "192": "./icons/icon192x192.png",
    "512": "./icons/icon512x512.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "index.js"
      ],
      "css": [
        "style.css"
      ]
    }
  ]
}